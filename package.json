{
  "name": "@itwin/mobileui-react",
  "version": "0.1.1-dev.2",
  "description": "Bentley iTwin Mobile UI module",
  "main": "lib/mobileui-react.js",
  "types": "lib/mobileui-react.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "copy:assets": "cpx \"./src/**/*.{*css,svg}\" ./lib && cpx \"./public/**/*\" ./lib/public",
    "build": "npm run images:gen && npm run copy:assets && tsc 1>&2",
    "clean": "rimraf lib",
    "lint": "tslint --project . 1>&2",
    "build:debug": "npm run build && ./syncToTestApp.sh",
    "images:gen": "rimraf src/mobileui-react/images-tsx && svgr --typescript src/mobileui-react/images --out-dir src/mobileui-react/images-tsx --no-svgo --no-prettier --memo --svg-props className=\"core-icons-svgSprite\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/MobileUI-react"
  },
