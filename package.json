    "@bentley/imodeljs-quantity": "^2.19.2",
    "@bentley/orbitgt-core": "^2.19.2",
    "@bentley/presentation-common": "^2.19.2",
    "@bentley/presentation-frontend": "^2.19.2",
    "@bentley/product-settings-client": "^2.19.2",
    "@bentley/rbac-client": "^2.19.2",
    "@bentley/telemetry-client": "^2.19.2",
    "@bentley/ui-abstract": "^2.19.2",
    "@bentley/ui-components": "^2.19.2",
    "@bentley/ui-core": "^2.19.2",
    "@bentley/ui-framework": "^2.19.2",
    "@bentley/ui-ninezone": "^2.19.2",
